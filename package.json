{
  "name": "mmo-phaser3",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Wing Lee <44917547+wingleeio@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "assets": "cp -R src/client/index.html src/client/assets dist/client",
    "parcel": "parcel build src/client/index.ts -d dist/client",
    "build": "npm-run-all clean tsc assets parcel update:db",
    "start": "node dist/server/index.js",
    "dev": "nodemon --watch src -e ts --exec npm-run-all build start",
    "proto:generate": "protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" -I=./src/shared/protobuf --ts_out=\"./src/shared/protobuf\" --js_out=\"import_style=commonjs,binary:./src/shared/protobuf\" ./src/shared/protobuf/schema.proto",
    "update:db": "prisma generate"
  },
  "dependencies": {
    "@geckos.io/phaser-on-nodejs": "^1.2.7",
    "@geckos.io/snapshot-interpolation": "^1.1.0",
    "@prisma/client": "^3.8.1",
    "@protobuf-ts/plugin": "^2.1.0",
    "@types/webfontloader": "^1.6.34",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "google-protobuf": "^3.19.1",
    "module-alias": "^2.2.2",
    "phaser": "^3.55.2",
    "phaser3-rex-plugins": "^1.1.65",
    "prisma": "^3.8.1",
    "web3": "^1.6.1",
    "webfontloader": "^1.6.28",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^17.0.8",
    "@types/ws": "^8.2.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.3",
    "ts-node": "^10.4.0",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.5.4"
  },
  "alias": {
    "@shared/*": "./dist/shared/$1"
  },
  "_moduleAliases": {
    "@shared": "dist/shared"
  },
  "engines": {
    "node": ">=16.3.0"
  }
}
